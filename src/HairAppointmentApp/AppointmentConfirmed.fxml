<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HairAppointmentApp.AppointmentConfirmedController">
   <children>
      <ImageView fitHeight="175.0" fitWidth="726.0" layoutX="-2.0" pickOnBounds="true">
         <image>
            <Image url="@images/greybackground.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="177.0" layoutX="14.0" layoutY="13.0" pickOnBounds="true">
         <image>
            <Image url="@images/hairicon_2.png" />
         </image>
      </ImageView>
      <Label alignment="CENTER" layoutX="201.0" layoutY="79.0" prefHeight="17.0" prefWidth="437.0" text="Hair Appointment System">
         <font>
            <Font name="Britannic Bold" size="32.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="170.0" layoutY="184.0" prefHeight="51.0" prefWidth="389.0" text="APPOINTMENT CONFIRMED ">
         <font>
            <Font name="Britannic Bold" size="29.0" />
         </font>
      </Label>
      <Label fx:id="appointmentMessageLabel" alignment="CENTER" layoutX="14.0" layoutY="318.0" prefHeight="70.0" prefWidth="701.0">
         <font>
            <Font name="System Bold Italic" size="13.0" />
         </font>
      </Label>
      <Button fx:id="exitButton" layoutX="324.0" layoutY="516.0" mnemonicParsing="false" onAction="#exitButtonAction" prefHeight="25.0" prefWidth="63.0" text="Exit">
